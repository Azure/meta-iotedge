---
 edgelet/iotedge/Cargo.toml | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/edgelet/iotedge/Cargo.toml b/edgelet/iotedge/Cargo.toml
index 5337bd54..e3ad0bfa 100644
--- a/edgelet/iotedge/Cargo.toml
+++ b/edgelet/iotedge/Cargo.toml
@@ -44,7 +44,7 @@ support-bundle = { path = "../support-bundle" }
 [target.'cfg(unix)'.dependencies]
 byte-unit = "3.0.3"
 libc = "0.2"
-sysinfo = "0.9.6"
+sysinfo = "0.12.0"
 
 [target.'cfg(windows)'.dependencies]
 winapi = { version = "0.3", features = ["ntdef", "ntstatus", "winnt", "winsock2"] }
